{
  "firebase": "cleanlifts",
  "public": "build",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/user/user.html"
    },
    {
      "source": "/user.js",
      "destination": "/user/user.js"
    },
    {
      "source": "/user.css",
      "destination": "/user/user.css"
    }
  ]
}
