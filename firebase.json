{
  "firebase": "cleanlifts",
  "public": "build",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "redirects": [
    {
      "source": "/login",
      "destination": "/login/",
      "type": 301
    },
    {
      "source": "/signup",
      "destination": "/signup/",
      "type": 301
    }
  ],

  "rewrites": [
    {
      "source": "/",
      "destination": "/user/user.html"
    },
    {
      "source": "/user.js",
      "destination": "/user/user.js"
    },
    {
      "source": "/user.css",
      "destination": "/user/user.css"
    }
  ]
}
