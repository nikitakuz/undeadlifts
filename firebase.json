{
  "firebase": "undeadlifts",
  "public": "build",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/user/user.html"
    },
    {
      "source": "/user.js",
      "destination": "/user/user.js"
    },
    {
      "source": "/user.css",
      "destination": "/user/user.css"
    },
    {
      "source": "/login",
      "destination": "/login/login.html"
    },
    {
      "source": "/login.js",
      "destination": "/login/login.js"
    },
    {
      "source": "/signup.js",
      "destination": "/signup/signup.js"
    }
  ]
}
